{
  "manifest_version": 3,
  "version": "1.0",
  "name": "CleanRead",
  "description": "NSFW words censorship",
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["badWords.json"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"]
    }
  ]
}
